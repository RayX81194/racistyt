{
  "manifest_version": 3,
  "name": "RacistYT",
  "version": "1.1",
  "description": "Youtube but hates white theme.",
  "action": {
        "default_popup": "popup.html"
    },
  "permissions": [
      "storage",
      "activeTab"
  ],
  "content_scripts": [
      {
          "matches": ["*://www.youtube.com/*"],
          "js": ["content.js"],
          "css": ["black-theme.css"]
      }
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
